{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Go",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${fileDirname}",
      "console": "integratedTerminal",
      "showLog": false,
      "trace": "verbose",
      "env": {
        "GO111MODULE": "on"
      }
    },
    {
      "name": "Python: Run main",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/main.py",
      "console": "integratedTerminal"
    }
  ]
}